                                                                            <div class="input-group">
                                                                                @Html.DropDownList("BossDocType", (IEnumerable<SelectListItem>)ViewBag.DocTypes, string.Empty,
                                                                           new
                                                                           {
                                                                               @class = "form-control edit-control main-control",
                                                                               @disabled = "disabled"
                                                                           })
